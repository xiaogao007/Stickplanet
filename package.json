{
  "name": "miaoda-taro-react-starter",
  "version": "1.0.0",
  "description": "开箱即用的基于Taro + React + Zustand + TailwindCSS + TypeScript的模板",
  "author": "miaoda-team",
  "license": "MIT",
  "templateInfo": {
    "name": "default",
    "typescript": true,
    "css": "sass"
  },
  "scripts": {
    "dev": "taro build --type h5 --watch",
    "dev:h5": "taro build --type h5 --watch",
    "dev:weapp": "taro build --type weapp --watch",
    "build": "taro build --type h5",
    "build:weapp": "taro build --type weapp",
    "postinstall": "weapp-tw patch",
    "lint": "npx biome check --write --unsafe --diagnostic-level=error || true; tsgo -p tsconfig.check.json; ./scripts/checkAuth.sh; ./scripts/checkNavigation.sh",
    "deploy:cloudfunctions": "node scripts/deploy-cloudfunctions.js"
  },
  "browserslist": [
    "last 5 years",
    "Android >= 9",
    "iOS >= 13",
    "not dead"
  ],
  "dependencies": {
    "@babel/runtime": "^7.28.3",
